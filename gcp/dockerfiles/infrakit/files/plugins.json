[
  {
    "Plugin": "manager",
    "Launch": {
      "os": {
        "Cmd": "infrakit-manager --name=group --proxy-for-group=group-stateless swarm > /infrakit/manager.log 2>&1"
      }
    }
  },
  {
    "Plugin": "flavor-combo",
    "Launch": {
      "os": {
        "Cmd": "infrakit-flavor-combo > /infrakit/flavor-combo.log 2>&1"
      }
    }
  },
  {
    "Plugin": "flavor-swarm",
    "Launch": {
      "os": {
        "Cmd": "infrakit-flavor-swarm --manager-init-template=file:///infrakit/manager-join.sh --worker-init-template=file:///infrakit/worker-join.sh > /infrakit/flavor-swarm.log 2>&1"
      }
    }
  },
  {
    "Plugin": "flavor-vanilla",
    "Launch": {
      "os": {
        "Cmd": "infrakit-flavor-vanilla > /infrakit/flavor-vanilla.log 2>&1"
      }
    }
  },
  {
    "Plugin": "group-default",
    "Launch": {
      "os": {
        "Cmd": "infrakit-group-default --name=group-stateless > /infrakit/group.log 2>&1"
      }
    }
  },
  {
    "Plugin": "instance-gcp",
    "Launch": {
      "os": {
        "Cmd": "infrakit-instance-gcp > /infrakit/instance-gcp.log 2>&1"
      }
    }
  }
]
