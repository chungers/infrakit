[
  {
    "Plugin": "manager",
    "Launch": {
      "os": {
        "Cmd": "infrakit-manager --name=group --proxy-for-group=group-stateless swarm --log=5 > /infrakit/manager.log 2>&1"
      }
    }
  },
  {
    "Plugin": "flavor-combo",
    "Launch": {
      "os": {
        "Cmd": "infrakit-flavor-combo --log=5 > /infrakit/flavor-combo.log 2>&1"
      }
    }
  },
  {
    "Plugin": "flavor-swarm",
    "Launch": {
      "os": {
        "Cmd": "infrakit-flavor-swarm --log=5 --manager-init-template=file:///infrakit/manager-join.sh --worker-init-template=file:///infrakit/worker-join.sh > /infrakit/flavor-swarm.log 2>&1"
      }
    }
  },
  {
    "Plugin": "flavor-vanilla",
    "Launch": {
      "os": {
        "Cmd": "infrakit-flavor-vanilla --log=5 > /infrakit/flavor-vanilla.log 2>&1"
      }
    }
  },
  {
    "Plugin": "group-default",
    "Launch": {
      "os": {
        "Cmd": "infrakit-group-default --log=5 --name=group-stateless --poll-interval=30s > /infrakit/group.log 2>&1"
      }
    }
  },
  {
    "Plugin": "instance-gcp",
    "Launch": {
      "os": {
        "Cmd": "infrakit-instance-gcp --log=5 > /infrakit/instance-gcp.log 2>&1"
      }
    }
  }
]
