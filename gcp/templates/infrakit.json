{
  "ID": "%s",
  "Properties": {
    "Allocation": {
      "%s": %s
    },
    "Instance": {
      "Plugin": "instance-gcp",
      "Properties": {
        "MachineType": "%s",
        "Network": "%s",
        "NamePrefix": "%s",
        "DiskSizeMb": %d,
        "DiskImage": "%s",
        "TargetPool": "%s",
        "Tags": ["swarm", "%s-node"],
        "Scopes": [
          "https://www.googleapis.com/auth/cloudruntimeconfig",
          "https://www.googleapis.com/auth/logging.write"
        ]
      }
    },
    "Flavor": {
      "Plugin": "flavor-combo",
      "Properties": {
        "Flavors": [
          {
            "Plugin": "flavor-vanilla",
            "Properties": {
              "Init": [%s]
            }
          },
          {
            "Plugin": "flavor-swarm",
            "Properties": {
              "Type": "%s",
              "DockerRestartCommand": "service docker restart"
            }
          }
        ]
      }
    }
  }
}
